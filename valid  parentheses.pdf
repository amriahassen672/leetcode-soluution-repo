class Solution(object):
    def isValid(self, s):
        bracket_types = {
         "(" : ")",
         "[" : "]",
         "{" : "}"
        }
        stack = []
        for char in s:
            if char in bracket_types:
                stack.append(char)
        
            else:
                if not stack or bracket_types[stack.pop()] != char:
                 
                    return False
        return not stack   
